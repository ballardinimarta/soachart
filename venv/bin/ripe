#!/Users/marta.ballardini/Desktop/charttests/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'ripe==0.0.4','console_scripts','ripe'
__requires__ = 'ripe==0.0.4'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('ripe==0.0.4', 'console_scripts', 'ripe')()
    )
